{
  "name": "design-portfolio",
  "description": "A website to showcase my design work",
  "version": "3.0.0",
  "private": true,
  "author": "Brett Stevenson <bstevensondev@gmail.com> (https://brettstevenson.io)",
  "scripts": {
    "develop": "gatsby develop",
    "dev": "gatsby develop -o",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "lint": "eslint . --ext .js,.jsx --ignore-path .gitignore",
    "lint:fix": "eslint . --ext .js,.jsx --fix --ignore-path .gitignore",
    "format": "prettier \"**/*.{md,mdx} \" --write",
    "lint:ci": "yarn lint --format junit -o results/eslint/result.xml",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "cy:run:ci": "cypress run --browser chrome --reporter junit --reporter-options 'mochaFile=results/cypress/result.xml'",
    "test:e2e:dev": "cross-env CYPRESS_SUPPORT=y start-server-and-test dev http://localhost:8000 cy:open",
    "test:e2e:run": "cross-env CYPRESS_SUPPORT=y start-server-and-test develop http://localhost:8000 cy:run",
    "test:e2e:ci": "cross-env CYPRESS_SUPPORT=y start-server-and-test develop http://localhost:8000 cy:run:ci"
  },
  "dependencies": {
    "@fortawesome/fontawesome": "^1.1.8",
    "@fortawesome/fontawesome-free-regular": "^5.0.13",
    "@fortawesome/fontawesome-free-solid": "^5.0.13",
    "@fortawesome/fontawesome-svg-core": "^1.2.16",
    "@fortawesome/free-brands-svg-icons": "^5.7.1",
    "@fortawesome/free-regular-svg-icons": "^5.7.1",
    "@fortawesome/free-solid-svg-icons": "^5.7.1",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "@mdx-js/mdx": "^1.0.15",
    "@mdx-js/react": "^1.0.15",
    "@reach/dialog": "^0.2.8",
    "babel-plugin-styled-components": "^1.10.0",
    "gatsby": "^2.13.41",
    "gatsby-cli": "2.6.2",
    "gatsby-image": "^2.0.41",
    "gatsby-mdx": "^0.6.2",
    "gatsby-plugin-catch-links": "^2.0.13",
    "gatsby-plugin-google-analytics": "^2.0.18",
    "gatsby-plugin-lodash": "^3.0.5",
    "gatsby-plugin-manifest": "^2.2.0",
    "gatsby-plugin-netlify": "^2.0.15",
    "gatsby-plugin-offline": "^2.0.25",
    "gatsby-plugin-react-helmet": "^3.0.12",
    "gatsby-plugin-sharp": "^2.0.35",
    "gatsby-plugin-sitemap": "^2.0.12",
    "gatsby-plugin-styled-components": "^3.0.7",
    "gatsby-plugin-transition-link": "^1.12.3",
    "gatsby-remark-external-links": "^0.0.4",
    "gatsby-remark-images": "^3.0.11",
    "gatsby-remark-responsive-iframe": "^2.1.1",
    "gatsby-source-filesystem": "^2.0.32",
    "gatsby-transformer-sharp": "^2.1.18",
    "gsap": "^2.1.2",
    "lodash": "^4.17.11",
    "polished": "^3.2.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.0",
    "react-icons": "^3.6.1",
    "react-spring": "^8.0.19",
    "styled-components": "^4.1.3",
    "styled-icons": "^7.14.0",
    "styled-theming": "^2.2.0",
    "tailwind.macro": "^0.5.10",
    "tailwindcss": "^0.7.3"
  },
  "keywords": [
    "gatsby",
    "gatsby-starter",
    "gatsby-starter-portfolio",
    "lekoarts"
  ],
  "repository": {
    "url": "https://github.com/tterb/gatsby-design-portfolio",
    "type": "git"
  },
  "bugs": {
    "url": "https://github.com/tterb/gatsby-design-portfolio/issues"
  },
  "homepage": "https://tterb-design.netlify.com",
  "license": "MIT",
  "main": "n/a",
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "cross-env": "^5.2.0",
    "cypress": "^3.2.0",
    "cypress-testing-library": "^2.6.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.2.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "gatsby-cypress": "^0.1.9",
    "prettier": "^1.17.0",
    "start-server-and-test": "^1.9.0"
  }
